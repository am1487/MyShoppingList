<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>com.myshoppinglist.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.myshoppinglist.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjaxRequest</servlet-name>
        <servlet-class>com.myshoppinglist.AjaxRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calculate</servlet-name>
        <servlet-class>com.myshoppinglist.Calculate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/UserLogin/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxRequest</servlet-name>
        <url-pattern>/AjaxRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calculate</servlet-name>
        <url-pattern>/Calculate</url-pattern>
    </servlet-mapping>


</web-app>
